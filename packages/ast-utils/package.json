{
  "name": "@unminify-kit/ast-utils",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.6.3",
  "description": "ðŸ”ªðŸ“¦ Unminify and beautify bundled code",
  "author": "Pionxzh",
  "license": "MIT",
  "sideEffects": false,
  "main": "src/index.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap --clean",
